        <servlet-name>writepost</servlet-name>
        <url-pattern>/writepost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>photo</servlet-name>
        <url-pattern>/photo</url-pattern>
    </servlet-mapping>
